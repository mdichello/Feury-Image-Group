<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_customer_pricelist" model="ir.sequence">
            <field name="name">Customer Pricelist</field>
            <field name="code">customer.pricelist</field>
            <field name="active">True</field>
            <field name="prefix">P</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_customer_product_pricelist" model="ir.sequence">
            <field name="name">Customer Product Pricelist</field>
            <field name="code">product.pricelist</field>
            <field name="active">True</field>
            <field name="prefix">CPP</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
